#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,sum=0;
    scanf("%d",&n);
    int *arr = malloc(n*sizeof(int));
    for(int j=0;j<n;j++)
    {
        scanf("%d",&arr[j]);
        sum+=arr[j];
    }
    printf("%d",sum);
    free(arr);
    
    return 0;
}
